{
  "expo": {
    "name": "Stylo Artificial",
    "slug": "stylo-artificial",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "@react-native-community/datetimepicker",
      "expo-web-browser"
    ],
    "orientation": "portrait",
    "icon": "./assets/logo/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stylo-ai.styloartificial"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.stylo_ai.styloartificial"
    },
    "extra": {
      "eas": {
        "projectId": "44938cb2-1206-4cb4-9154-f6c41d1be23c"
      }
    }
  }
}
